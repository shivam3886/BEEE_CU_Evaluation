void setup()
{
  pinMode(7,OUTPUT);
  pinMode(10,OUTPUT);
  pinMode(9,INPUT);
  pinMode(5,OUTPUT);
}

void loop()
{
  if(digitalRead(9) == LOW)
  {
    digitalWrite(7,HIGH);
    delay(1000);
    digitalWrite(7,LOW);
  }
 
  while(digitalRead(9) == LOW)
  {
     digitalWrite(10,HIGH);
    delay(100);
    digitalWrite(10,LOW);
    digitalWrite(5,HIGH);
    delay(100);
    digitalWrite(5,LOW);
    
  }
}
